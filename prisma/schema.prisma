generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model WorkExperience {
  id          Int     @id @default(autoincrement())
  company     String
  role        String
  duration    String
  skills      String?
  description String?
  icon        String?
}
